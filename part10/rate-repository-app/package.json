    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.4",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "^49.0.0",
    "react-test-renderer": "^18.2.0"
  },
  "private": true
